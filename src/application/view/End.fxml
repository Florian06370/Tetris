<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>

<Pane fx:id="EndPane" prefHeight="700.0" prefWidth="840.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.EndController">
   <children>
      <Pane fx:id="barPane" minHeight="700.0" minWidth="840.0" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: rgb(0,0,0,0.6);">
         <children>
            <Label fx:id="blueScore" layoutX="275.0" layoutY="561.0" textFill="rgb(11, 66, 155)">
               <font>
                  <Font name="Consolas Bold" size="50.0" />
               </font>
            </Label>
            <Label fx:id="yellowScore" layoutX="375.0" layoutY="561.0" textFill="rgb(237,157, 0)">
               <font>
                  <Font name="Consolas Bold" size="50.0" />
               </font>
            </Label>
            <Label fx:id="redScore" layoutX="475.0" layoutY="561.0" textFill="rgb(155, 11, 66)">
               <font>
                  <Font name="Consolas Bold" size="50.0" />
               </font>
            </Label>
            <Label fx:id="greenScore" layoutX="575.0" layoutY="561.0" textFill="rgb(66,155, 11)">
               <font>
                  <Font name="Consolas Bold" size="50.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Pane fx:id="center" layoutY="50.0" minHeight="300.0" minWidth="840.0" prefHeight="200.0" prefWidth="200.0">
         <children>
            <TextFlow fx:id="title" minWidth="700.0" prefHeight="300.0" prefWidth="840.0" textAlignment="CENTER">
               <children>
                  <Text fx:id="mainTitle" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0">
                     <font>
                        <Font name="Consolas Bold" size="30.0" />
                     </font>
                  </Text>
               </children>
            </TextFlow>
         </children>
      </Pane>
      <Button fx:id="backBtn" layoutX="6.0" layoutY="11.0" maxHeight="40.0" maxWidth="60.0" minHeight="20.0" minWidth="60.0" mnemonicParsing="false" text="Back" />
   </children>
</Pane>
